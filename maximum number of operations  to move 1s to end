class Solution {
public:
    int maxOperations(string s) {
        int opr=0;
        int ones_cnt=0;
        for(int i=0;i<s.length();i++){
            if(s[i]=='1'){
                ones_cnt++;
            }else if
            (i>0 && s[i-1]=='1'){
                opr+=ones_cnt;
            }

            
        }
        return opr;
        
    }
};
